<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/button5"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:text="Valider"
        android:onClick="validerCSAP"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="431dp" />

    <TextView
        android:id="@+id/textView9"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="72dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Sondage xx"
        android:textSize="14pt" />

    <TextView
        android:id="@+id/textView10"
        tools:layout_editor_absoluteX="110dp"
        tools:layout_editor_absoluteY="141dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Description de ce sondage" />

    <Button
        android:id="@+id/account_modification2"
        android:layout_width="@dimen/large_padding"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Proposition 1"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="208dp" />

    <Button
        android:id="@+id/friends_modification2"
        android:layout_width="@dimen/large_padding"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Proposition 2"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="271dp" />

    <Button
        android:id="@+id/questionnaires2"
        android:layout_width="@dimen/large_padding"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Proposition 3"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="339dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Retour"
        android:onClick="modifCSAP"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="16dp" />
</android.support.v7.widget.LinearLayoutCompat>